    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": false,
        "noImplicitAny": true,
        "strict": true,
        "module": "es6",
        "moduleResolution": "node",
        "allowUnreachableCode": true,
        "target": "es6",
        "noEmitOnError": true
    },
    "include": [
        "./src/*"
    ]
}
